{
    "name": "networking/form-generator-bundle",
    "description": "This Bundle adds a form generator to the init cms bundle",
    "keywords": ["symfony", "form generator"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yorkie Chadwick",
            "homepage": "http://web.networking.ch",
            "email": "y.chadwick@networking.ch"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.8.*",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "networking/init-cms-bundle": "^2.8.5"
    },
    "autoload": {
        "psr-4": {"Networking\\FormGeneratorBundle\\": ""}
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
