{
    "name": "networking/form-generator-bundle",
    "description": "This Bundle adds a form generator to the init cms bundle",
    "keywords": ["symfony", "form generator"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yorkie Chadwick",
            "homepage": "http://web.networking.ch",
            "email": "y.chadwick@networking.ch"
        }
    ],
    "require": {
        "php": "^8.1",
        "networking/init-cms-bundle": "^6.2 || dev-master",
        "friendsofsymfony/rest-bundle": "^3.4",
        "symfony/config": "^6.0",
        "phpoffice/phpspreadsheet": "^1.4"
    },
    "autoload": {
        "psr-4": {"Networking\\FormGeneratorBundle\\": "src"}
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
